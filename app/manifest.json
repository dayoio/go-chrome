{
    "name": "__MSG_appName__",
    "version": "20",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "zh_CN",
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "content_scripts": [
        {
            "js": [
                "scripts/contentscript.js"
            ],
            "matches": [
                "http://www.iqiyi.com/*"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "browser_action": {
      "default_icon": {
        "16": "images/icon-16.png"
      }
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "web_accessible_resources": []
}
